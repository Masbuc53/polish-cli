{
  "vault": {
    "path": "/Users/developer/ObsidianVault/Development",
    "structure": {
      "documents": "Documentation",
      "media": "Assets",
      "code": "Code",
      "references": "References"
    }
  },
  "originals": {
    "path": "/Users/developer/OrganizedFiles/Development",
    "organizationStyle": "project-based",
    "createYearFolders": false,
    "preserveStructure": true
  },
  "sources": [
    {
      "path": "/Users/developer/Code",
      "includeSubfolders": true,
      "excludePatterns": ["node_modules", "*.log", "*.tmp", ".git", "dist", "build"]
    },
    {
      "path": "/Users/developer/Downloads",
      "includeSubfolders": false,
      "excludePatterns": ["*.tmp", "*.partial"]
    },
    {
      "path": "/Users/developer/Documentation",
      "includeSubfolders": true,
      "excludePatterns": ["*.cache", "*.tmp"]
    }
  ],
  "processing": {
    "extractText": true,
    "maxFileSize": "25MB",
    "supportedFormats": [
      "js", "ts", "jsx", "tsx", "py", "java", "cpp", "c", "go", "rs", "rb", "php", "cs", "swift", "kt",
      "html", "css", "scss", "sass", "less", "vue", "svelte",
      "json", "xml", "yaml", "yml", "toml", "ini", "env", "cfg",
      "md", "txt", "rst", "adoc",
      "png", "jpg", "jpeg", "gif", "bmp", "svg", "webp", "ico",
      "zip", "tar", "gz", "rar", "7z"
    ]
  },
  "tagging": {
    "maxTags": 10,
    "autoGenerateTypeTags": true,
    "autoGenerateDateTags": false,
    "autoGenerateSourceTags": true,
    "customTagPatterns": {
      "language": "javascript|typescript|python|java|cpp|go|rust|ruby|php|csharp|swift|kotlin",
      "framework": "react|vue|angular|django|flask|spring|express|nextjs|nuxt|svelte",
      "project": "extract from directory name",
      "category": "frontend|backend|fullstack|mobile|desktop|cli|library|api",
      "status": "active|archived|deprecated|experimental",
      "technology": "database|cloud|devops|testing|security|performance"
    },
    "excludeTags": ["temp", "cache", "tmp", "node_modules", "build", "dist"]
  },
  "api": {
    "mode": "claude-code",
    "timeout": 30000,
    "retries": 3,
    "systemPrompt": "You are analyzing software development files. Focus on programming languages, frameworks, project context, and technical concepts. Generate tags that would help a developer organize their code and documentation."
  }
}